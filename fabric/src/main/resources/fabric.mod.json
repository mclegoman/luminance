{
  "schemaVersion": 1,
  "id": "luminance",
  "version": "${version}",
  "name": "Luminance",
  "description": "Luminance is a library mod that helps with shader rendering, and contains helper classes, and event hooks to help other mods with game rendering.",
  "authors": [
    "dannytaylor",
    "Nettakrim"
  ],
  "contributors": [
  ],
  "contact": {
    "sources": "https://github.com/MCLegoMan/Luminance",
    "issues": "https://github.com/MCLegoMan/Luminance/issues"
  },
  "license": [
    "LGPL-3.0-or-later"
  ],
  "icon": "assets/luminance/icons/icon.png",
  "environment": "client",
  "entrypoints": {
    "luminance_client": [
      "com.mclegoman.luminance.modloader.client.LuminanceFabricLoader"
    ],
    "modmenu": [
	    "com.mclegoman.luminance.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "dt-luminance_entrypoints.mixins.json",
    "dt-luminance_fabric.mixins.json",
    "dt-luminance_common.mixins.json"
  ],
  "depends": {
    "minecraft": [
      "1.21.4"
    ],
    "fabric-resource-loader-v0": "*",
    "fabric-key-binding-api-v1": "*",
    "fabric-lifecycle-events-v1": "*",
    "java": ">=21"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "links": {
        "mclegoman.donate": "https://ko-fi.com/mclegoman"
      }
    }
  }
}